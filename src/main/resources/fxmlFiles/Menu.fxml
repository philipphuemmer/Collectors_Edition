<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="697.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
   <children>
      <VBox id="VBoxLeft" prefHeight="480.0" prefWidth="212.0" stylesheets="@../cssFiles/Menu.css">
         <children>
            <Pane prefHeight="91.0" prefWidth="212.0" />
            <Button fx:id="newCollectionButton" mnemonicParsing="false" prefHeight="52.0" prefWidth="226.0" stylesheets="@../cssFiles/Menu.css" text="New Collection" textFill="WHITE">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="newScrapbookButton" mnemonicParsing="false" onAction="#newScrapbook" prefHeight="52.0" prefWidth="226.0" stylesheets="@../cssFiles/Menu.css" text="New Scrapbook" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="openButton" mnemonicParsing="false" onAction="#open" prefHeight="52.0" prefWidth="226.0" stylesheets="@../cssFiles/Menu.css" text="Open" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="galleryButton" mnemonicParsing="false" prefHeight="52.0" prefWidth="226.0" stylesheets="@../cssFiles/Menu.css" text="Gallery" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <VBox id="VBoxRight" alignment="TOP_CENTER" layoutX="212.0" prefHeight="480.0" prefWidth="485.0" stylesheets="@../cssFiles/Menu.css">
         <children>
            <ListView id="scrapbookNames" fx:id="scrapbookNames" minHeight="-Infinity" prefHeight="419.0" prefWidth="485.0" stylesheets="@../cssFiles/Menu.css" />
            <Pane prefHeight="14.0" prefWidth="485.0" />
            <HBox alignment="TOP_CENTER" prefHeight="28.0" prefWidth="485.0">
               <children>
                  <Button id="openScrapbookButton" fx:id="openScrapbook" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#openScrapbook" prefHeight="25.0" prefWidth="150.0" stylesheets="@../cssFiles/Menu.css" text="Open Scrapbook" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets right="25.0" />
                     </HBox.margin></Button>
                  <Button id="deleteScrapbookButton" fx:id="deleteScrapbook" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#deleteScrapbook" prefHeight="25.0" prefWidth="150.0" stylesheets="@../cssFiles/Menu.css" text="Delete" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets left="25.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="TOP_RIGHT" prefHeight="38.0" prefWidth="697.0">
         <children>
            <Button id="minimizeButton" fx:id="minimizeButton" mnemonicParsing="false" onAction="#minimizeApplication" prefHeight="35.0" prefWidth="35.0" stylesheets="@../cssFiles/Menu.css" text="-" />
            <Button id="closeButton" fx:id="closeButton" mnemonicParsing="false" onAction="#closeApplication" prefHeight="35.0" prefWidth="35.0" stylesheets="@../cssFiles/Menu.css" text="X" />
         </children>
      </HBox>
   </children>
</AnchorPane>
